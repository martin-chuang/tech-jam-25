# Development Environment Variables for Docker
# These are automatically loaded by docker-compose-dev.yml

# Flask Configuration
FLASK_ENV=development
FLASK_DEBUG=1
SECRET_KEY=dev-secret-key
DEBUG=true
PORT=5000

# Database Configuration
DB_HOST=postgres
DB_PORT=5432
DB_USERNAME=postgres
DB_PASSWORD=postgres
DB_DATABASE=tiktok_techjam_dev

# Redis Configuration
REDIS_HOST=redis
REDIS_PORT=6379

# OpenAI Configuration (add your API key)
OPENAI_API_KEY=your-openai-api-key-here

# Logging
LOG_LEVEL=INFO

# CORS Configuration
CORS_ORIGINS=http://localhost:3000,http://127.0.0.1:3000
